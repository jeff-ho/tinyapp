<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a> 
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <div style="display:flex; align-items: inherit">
      <% if(user){ %>
        <p style="font-size:x-large; padding-top: 15px;">Logged in as <%= user.email %>! </p>
        <form method="POST" action="/logout">
          <button type="submit" class="btn btn-dark" style="height: 45px; width: 150px; margin-left: 20px;" >Logout</button>
        </form>
      <% } else{ %>  
        <a href="/login" style="color:antiquewhite; margin-right: 9px;">Log In</a>
        <a href="/register" style="color:antiquewhite">Register</a>
     <% } %>
    </div>
  </nav>
</header>

